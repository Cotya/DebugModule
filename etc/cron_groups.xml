<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Cron/etc/cron_groups.xsd">
    <group id="debug">
        <schedule_generate_every>1</schedule_generate_every>
        <schedule_ahead_for>5</schedule_ahead_for>
        <schedule_lifetime>15</schedule_lifetime>
        <history_cleanup_every>1</history_cleanup_every>
        <history_success_lifetime>0</history_success_lifetime>
        <history_failure_lifetime>0</history_failure_lifetime>
        <use_separate_process>0</use_separate_process>
    </group>
</config>
